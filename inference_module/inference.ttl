PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX crm: <http://www.cidoc-crm.org/cidoc-crm/>
PREFIX dlp: <http://www.ontologydesignpatterns.org/ont/dlp/dlp.owl#>
PREFIX lrm: <http://iflastandards.info/ns/lrm/lrmoo/>
PREFIX crminf: <http://www.ics.forth.gr/isl/CRMinf/>
PREFIX dl: <http://www.ontologydesignpatterns.org/ont/dlp/DOLCE-Lite.owl#>
PREFIX dns: <http://www.ontologydesignpatterns.org/ont/dlp/ExtendedDnS.owl#>
PREFIX gd: <https://data.golemlab.eu/entity/>
prefix gc: <https://ontology.golemlab.eu/>

gd:statement crminf:J4i_is_subject_of gd:belief .
gd:belief crminf:J4_that gd:statement .

gd:statement a crminf:I4_Proposition_Set .
gd:belief a crminf:I2_Belief .

gd:belief crminf:J2i_was_concluded_by gd:inference .
gd:inference crminf:J2_concluded_that gd:belief .

gd:inference a crminf:I5_Inference_Making .

gd:inference crminf:J3_applies gd:inferenceMethod .
gd:inferenceMethod crminf:J3i_was_applied_by gd:inference .

gd:inferenceMethod crm:P2_has_type gd:inferenceMethodType .
gd:inferenceMethodType crm:P2i_is_type_of gd:inferenceMethod .

gd:inferenceMethod a crminf:I3_Inference_Logic .

gd:inferenceMethodType a gc:inferenceMethod .
gc:inferenceMethod rdfs:subClassOf crm:E55_Type .

gd:inference crm:P16_used_specific_object gd:evidence .
gd:evidence crm:P16i_was_used_for gd:inference .

gd:evidence a lrm:F2_Expression .
gd:evidence crm:P2_has_type gd:evidenceType .
gd:evidenceType crm:P2i_is_type_of gd:evidence .

gd:evidenceType a gc:evidenceType .
gc:evidenceType rdfs:subClassOf crm:E55_Type .

gd:evidence lrm:R5i_is_component_of gd:source .
gd:source lrm:R5_has_component gd:evidence .

gd:source a lrm:F2_Expression .

gd:inference crminf:J1_used_as_premise gd:premise .
gd:premise crminf:J1i_was_premise_for gd:inference .

gd:premise crm:P2_has_type gd:premiseType .
gd:premiseType crm:P2i_is_type_of gd:premise .

gd:premise a crminf:I2_Belief .
gd:premiseType a gc:premiseType .
gc:premiseType rdfs:subClassOf crm:E55_Type .

gd:premise crminf:J4_that gd:premiseStatement .
gd:premiseStatement crminf:J4i_is_subject_of gd:premise .

gd:premiseStatement a crminf:I4_Proposition_Set .
