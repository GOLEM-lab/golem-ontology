PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX crm: <http://www.cidoc-crm.org/cidoc-crm/>
PREFIX dlp: <http://www.ontologydesignpatterns.org/ont/dlp/dlp.owl#>
PREFIX dl: <http://www.ontologydesignpatterns.org/ont/dlp/DOLCE-Lite.owl#>
PREFIX dns: <http://www.ontologydesignpatterns.org/ont/dlp/ExtendedDnS.owl#>
PREFIX gd: <https://data.golemlab.eu/entity/>
PREFIX gc: <https://ontology.golemlab.eu/>

gd:relationship dns:involves gd:character .
gd:character dns:involved-in gd:relationship .

gd:relationship dns:d-uses gd:relationshipHistory .
gd:relationshipHistory dns:d-used-by gd:relationship .

gd:relationship dns:d-uses gd:role .
gd:role dns:d-used-by gd:relationship .

gd:relationshipHistory dns:sequences gd:relationshipEvent .
gd:relationshipEvent dns:sequenced-by gd:relationshipHistory .

gd:character dns:plays gd:role .
gd:role dns:played-by gd:character .

gd:relationship a gc:social-relationship .

gd:relationshipType a gc:relationshipType .
gd:character a gc:G1_Character .

gc:G1_Character rdfs:subClassOf crm:E89_Propositional_Object .
gc:G1_Character rdfs:subClassOf dns:agentive-social-object .

gd:role crm:P2_has_type gd:relationshipRole .
gd:relationshipRole crm:P2i_is_type_of gd:role .


gd:relationshipRole a gc:relationshipRole .
gc:relationshipRole rdfs:subClassOf crm:E55_Type .
gc:relationshipType rdfs:subClassOf crm:E55_Type .


gd:relationshipHistory a dlp:course .
gd:relationshipEvent a dl:perdurant .

gd:relationship crm:P2_has_type gd:relationshipType .
gd:relationshipType crm:P2i_is_type_of gd:relationship .
